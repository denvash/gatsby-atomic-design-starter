{
  "name": "dashboard",
  "description": "Hkube dashboard",
  "version": "1.0.0",
  "author": "Dennis Vash <dennisvash@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "tailwind",
    "storybook",
    "styled-components",
    "css-in-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kube-HPC/dashboard"
  },
  "bugs": {
    "url": "https://github.com/kube-HPC/hkube/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "pre-push": ""
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -o -p 8080",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "lint:css": "stylelint './src/**/*.js'",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "storybook": "NODE_ENV=production start-storybook -p 6006 -s public",
    "build-storybook": "yarn build; NODE_PATH=src/ NODE_ENV=production build-storybook -s public --quiet"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.0-beta.0",
    "framer-motion": "^1.9.1",
    "gatsby": "^2.19.17",
    "object.fromentries": "^2.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "redux-logger": "^3.0.6",
    "reusable": "^1.0.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.0",
    "styled-tools": "^1.7.1",
    "typeface-rajdhani": "^0.0.72"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@dennisvash/eslint-config": "^2.2.2",
    "@dennisvash/prettier-config": "^1.0.0",
    "@storybook/react": "^5.3.8",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.2.29",
    "core-js": "2.6.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-manifest": "^2.2.38",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-styled-components": "^3.1.18",
    "husky": "3.1.0",
    "lint-staged": "^9.5.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prop-types": "^15.7.2",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "twin.macro": "^1.0.0-alpha.7"
  }
}
